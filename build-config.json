{
    "extends": "react",
    "entries": {
        "index": "src/index.jsx"
    },
    "pages": {
        "index": {
            "template": "src/index.html",
            "entries": "index",
            "path": ""
        }
    },
    "transforms": {
        "stylus": "stylus",
        "styl": {
            "transformer": "stylus",
            "config": {
                "cssOptions": {
                    "modules": true,
                    "sourceMap": true,
                    "localIdentName": "[local]__[hash:base64:6]"
                }
            }
        }
    }
}
