{
    "name": "react-stater",
    "version": "1.0.0",
    "scripts": {
        "lint": "npm run lint:es",
        "lint:es": "eslint . --ext .js,jsx",
        "test": "jest --coverage --colors",
        "format": "npm run lint:es -- --fix",
        "clean": "rm -rf dist",
        "ci": "npm run clean && npm run lint && npm run test && npm run build",
        "start": "webpack-dev-server --config config/dev.js --progress --profile --watch",
        "build": "webpack --config config/prod.js --progress --profile --bail",
        "codecov": "codecov"
    },
    "dependencies": {
        "classnames": "^2.2.5",
        "intercept-fetch": "^1.3.3",
        "lodash-es": "^4.17.4",
        "mobx": "^3.2.2",
        "mobx-react": "^4.2.2",
        "moment": "^2.18.1",
        "normalize.css": "^7.0.0",
        "omit.js": "^1.0.0",
        "prop-types": "^15.5.10",
        "react": "^15.6.1",
        "react-dom": "^15.6.1",
        "react-redux": "^5.0.6",
        "react-router-dom": "^4.2.0",
        "redux": "^3.7.2",
        "redux-saga": "^0.15.6",
        "redux-saga-routines": "^1.1.2",
        "uuid": "^3.1.0"
    },
    "devDependencies": {
        "@types/enzyme": "^2.8.6",
        "@types/jest": "^20.0.7",
        "@types/prop-types": "^15.5.1",
        "@types/react": "^16.0.5",
        "@types/react-dom": "^15.5.4",
        "@types/react-router-dom": "^4.0.7",
        "@types/react-test-renderer": "^15.5.4",
        "@types/uuid": "^3.4.1",
        "@types/webpack": "^3.0.10",
        "babel-eslint": "^7.2.3",
        "babel-plugin-syntax-dynamic-import": "^6.18.0",
        "babel-plugin-transform-class-properties": "^6.24.1",
        "babel-plugin-transform-decorators-legacy": "^1.3.4",
        "babel-plugin-transform-runtime": "^6.23.0",
        "babel-preset-env": "^1.6.0",
        "babel-preset-react": "^6.24.1",
        "babel-preset-stage-1": "^6.24.1",
        "codecov": "^2.3.0",
        "enzyme": "^2.9.1",
        "eslint": "^4.5.0",
        "eslint-config-airbnb": "^15.1.0",
        "eslint-plugin-import": "^2.7.0",
        "eslint-plugin-jsx-a11y": "^5.1.1",
        "eslint-plugin-prettier": "^2.2.0",
        "eslint-plugin-react": "^7.3.0",
        "husky": "^0.14.3",
        "identity-obj-proxy": "^3.0.0",
        "jest": "^20.0.4",
        "prettier": "^1.5.3",
        "react-hot-loader": "^3.0.0-beta.7",
        "react-test-renderer": "^15.6.1",
        "typescript": "^2.4.2"
    },
    "jest": {
        "testRegex": "(/tests/.*|\\.(test|spec))\\.(js|jsx)$",
        "testPathIgnorePatterns": [
            "<rootDir>/tests/mock.js"
        ],
        "transformIgnorePatterns": [
            "<rootDir>/node_modules/(?!lodash-es)"
        ],
        "moduleDirectories": [
            "node_modules",
            "src"
        ],
        "moduleNameMapper": {
            "\\.(png|eot|svg|ttf|woff|woff2)(\\?.+)?$": "<rootDir>/tests/mock.js",
            "\\.(css|styl)$": "identity-obj-proxy"
        },
        "coverageDirectory": "coverage",
        "moduleFileExtensions": [
            "js",
            "jsx"
        ]
    },
    "engines": {
        "node": ">=8.0.0",
        "npm": ">=5.0.0"
    }
}
