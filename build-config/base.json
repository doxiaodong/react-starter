{
    "extends": "react",
    "entries": {
        "lib": "src/lib.js",
        "index": "src/index.jsx"
    },
    "pages": {
        "index": {
            "template": "src/index.html",
            "entries": "index",
            "path": ""
        }
    },
    "transforms": {
        "scss": {
            "transformer": "sass",
            "config": {
                "modules": true
            }
        }
    },
    "optimization": {
        "extractVendor": "lib"
    }
}
